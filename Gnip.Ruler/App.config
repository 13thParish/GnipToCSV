<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<configSections>
		<sectionGroup name="settings">
			<section name="gnip.ruler" type="Gnip.Ruler.Common.GnipRulerConfSection, Gnip.Ruler" allowLocation="true" allowDefinition="Everywhere" />
		</sectionGroup>
	</configSections>
	
	<settings>
    <gnip.ruler
      source="Twitter"
      live="true">
    </gnip.ruler>
	</settings>
	
	<system.net>
